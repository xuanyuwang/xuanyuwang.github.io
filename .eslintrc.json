{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "indent": [2, "tab"]
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2021
    }
}